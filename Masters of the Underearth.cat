    <selectionEntry id="4e73-ba18-34a4-92f3" name="Alchemist" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="5b54-3b35-1515-3588" type="max"/>
