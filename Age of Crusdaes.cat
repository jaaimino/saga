        <cost name=" Points" typeId="67d0-6d3a-a0de-3174" value="1.0"/>
      </costs>
    </selectionEntry>
    <selectionEntry id="f754-6d18-1fbc-8324" name="Fire-thrower" hidden="false" collective="false" import="true" type="model">
