        <profile id="b9d8-bf8f-e264-dd50" name="Attila" publicationId="90fb-5671-0e32-356b" page="24" hidden="false" typeId="c212-f7f0-1b50-166d" typeName="Unit">
          <characteristics>
            <characteristic name="Saga Dice" typeId="21d8-0469-79eb-3da5">2</characteristic>
